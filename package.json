{
  "name": "@adobe/rum-distiller",
  "version": "1.19.0",
  "scripts": {
    "test": "node --test --experimental-test-coverage --test-reporter=lcov --test-reporter-destination=lcov.info --test-reporter=spec --test-reporter-destination=stdout --test-reporter=junit --test-reporter-destination=junit.xml",
    "test-ci": "npm run test",
    "lint": "eslint .",
    "docs": "npx jsdoc2md --files '*.js'  > API.md",
    "benchmark": "node test/benchmarks/index.js",
    "benchmark:filtering": "node test/benchmarks/filtering.bench.js",
    "benchmark:facets": "node test/benchmarks/facets.bench.js",
    "benchmark:grouping": "node test/benchmarks/grouping.bench.js",
    "benchmark:e2e": "node test/benchmarks/e2e.bench.js",
    "profile": "node test/benchmarks/profile.js",
    "semantic-release": "semantic-release",
    "semantic-release-dry": "semantic-release --dry-run --branches $CI_BRANCH"
  },
  "type": "module",
  "main": "index.js",
  "license": "Apache-2.0",
  "devDependencies": {
    "@adobe/eslint-config-helix": "3.0.10",
    "@esm-bundle/chai": "4.3.4-fix.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/npm": "12.0.2",
    "eslint": "9.37.0",
    "jsdoc-to-markdown": "9.1.2",
    "semantic-release": "24.2.8"
  },
  "lint-staged": {
    "*.js": "eslint",
    "*.cjs": "eslint"
  }
}
